<launch>
  <arg name="droneip" default="192.168.1.10" />
  <node name="ardrone_driver" pkg="ardrone_autonomy" type="ardrone_driver" output="screen" args="-ip $(arg droneip)">
    <param name="navdata_demo" value="False" />
    <param name="realtime_navdata" value="True" />
    <param name="realtime_video" value="True" />
    <param name="looprate" value="30" />
    <param name="CC_F_URL" value="file://${ROS_HOME}/camera_info/king_front.yaml"/>
  </node>
</launch>
